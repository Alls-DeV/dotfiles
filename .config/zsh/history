bool heights(int N, vector<int> Z, vector<int> &H) {
    vector<int> ans = Z;
    for (int i = 1; i < N - 1; i++) {
        if (ans[i] == ans[i - 1] && ans[i + 1] != ans[i]) {\
            ans[i] = -1;\
        }
    for (auto &x : ans) if (x == -1) x = 0;
    ans.push_back(0);
    bool flag = false;
    for (int i = 1; i < N; i++) {
        if (max(ans[i - 1], ans[i]) != Z[i - 1]) {\
            if (flag) {\
                return false;\
            }\
            ans.insert(ans.begin() + (i - 1), 0);
            ans.pop_back();\
            flag = true;
        //dbg(ans);
    H = ans;
    return true;
