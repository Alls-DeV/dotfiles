using namespace std;
void solve() {\
	int n; cin >> n;\
	vector<int> v(n); for (auto &x : v) cin >> x;
	map<int, vector<int>, greater<>> mp;
	for (int i = 0; i < n; i++) mp[v[i]].push_back(i);
	dbg(mp);
	for (auto &[ans, pos] : mp) {
		for (int i = 1; i < pos.size(); i++) {
			dbg(ans, pos);
			if (pos[i] - pos[i - 1] <= 2) {\
				cout << ans << '\n'; return;\
			}\
		}\
	}\
	int ans = 0;\
	for (int i = 1; i < n; i++)\
		ans = max(ans, min(v[i - 1], v[i]));\
	cout << ans << '\n';\
}\
